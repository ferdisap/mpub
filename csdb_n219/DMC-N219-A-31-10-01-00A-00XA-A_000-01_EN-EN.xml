      <controlIndicatorRepository>
        <controlIndicatorGroup>
          <internalRef internalRefId="fig-001" internalRefTargetType="irtt01">&#160;Engine Starting Panel</internalRef>
          <controlIndicatorSpec controlIndicatorNumber="01">
            <controlIndicatorKey>1</controlIndicatorKey>
            <controlIndicatorName>ENGINE START MODE SEL Selector</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">GPU/X-START</inlineSignificantData>: Electrical Power for starting is provided by the GPU or cross-side engine.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">BAT</inlineSignificantData>: Electrical power for starting is provided by the aircraft batteries.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">OFF</inlineSignificantData>: No electrical power for starting.</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
          <controlIndicatorSpec controlIndicatorNumber="01">
            <controlIndicatorKey>2</controlIndicatorKey>
            <controlIndicatorName>STARTER (Momentary) Switch (Left/Right)</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">ON</inlineSignificantData>: Initiates starting engine.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">DISC</inlineSignificantData>: Stops the starter during motoring.</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
          <controlIndicatorSpec controlIndicatorNumber="01">
            <controlIndicatorKey>3</controlIndicatorKey>
            <controlIndicatorName>IGNITION Switch (Left/Right)</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">AUTO</inlineSignificantData>: Allows an automatic ignition to be performed.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">MAN</inlineSignificantData>: Engine ignition is performed manually.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">OFF</inlineSignificantData>: Ignition off.</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
        </controlIndicatorGroup>
        <controlIndicatorGroup>
          <internalRef internalRefId="fig-002" internalRefTargetType="irtt01">&#160;Propeller Control Panel</internalRef>
          <controlIndicatorSpec controlIndicatorNumber="02">
            <controlIndicatorKey>1</controlIndicatorKey>
            <controlIndicatorName>AUTO FTR Selector Switch</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">ARM</inlineSignificantData>: Selects the autofeather to be armed.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">OFF</inlineSignificantData>: off.</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
          <controlIndicatorSpec controlIndicatorNumber="02">
            <controlIndicatorKey>2</controlIndicatorKey>
            <controlIndicatorName>AUTO FTR TEST Switch</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">L(R)</inlineSignificantData>: Initiates L(R) autofeather test.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">OFF</inlineSignificantData>: off</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
          <controlIndicatorSpec controlIndicatorNumber="02">
            <controlIndicatorKey>3</controlIndicatorKey>
            <controlIndicatorName>GOVERNOR OVSP TEST Switch</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">L(R)</inlineSignificantData>: Initiates L(R) autofeather test.</controlIndicatorFunction>
              <controlIndicatorFunction><inlineSignificantData significantParaDataType="psd10">OFF</inlineSignificantData>: off.</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
          <controlIndicatorSpec controlIndicatorNumber="02">
            <controlIndicatorKey>4</controlIndicatorKey>
            <controlIndicatorName>EHMS TRACK Switch</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction>INOPERATIVE</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
          <controlIndicatorSpec controlIndicatorNumber="02">
            <controlIndicatorKey>5</controlIndicatorKey>
            <controlIndicatorName>PROP SYNC Switch</controlIndicatorName>
            <controlIndicatorDescr>
              <controlIndicatorFunction>INOPERATIVE</controlIndicatorFunction>
            </controlIndicatorDescr>
          </controlIndicatorSpec>
        </controlIndicatorGroup>
