    <commonRepository>
      <figure id="fig-001">
        <title>Engine Starting Panel</title>
        <graphic reproductionHeight="130mm" infoEntityIdent="ICN-0001Z-00005-001-01.png"></graphic>
      </figure>
      <figure id="fig-002">
