                <response><para>ABORTED</para></response>
              </challengeAndResponse>
            </crewDrillStep>
          </crewDrill>
        </levelledPara>
        <levelledPara>
          <title>Immediatelly After Take-off</title>
          <crewDrill orderedStepsFlag="1">
            <para>Indication: TBD</para>
            <crewDrillStep>
              <challengeAndResponse>
                <challenge><para>Throttle</para></challenge>
                <crewMemberGroup>
                  <crewMember crewMemberType="cm02"/>
